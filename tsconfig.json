{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "sourceMap": true,
    "incremental": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true
  },
}
