    "name": {
      "title": "Node Name",
      "type": "string",
      "ui:widget": "node_name"
    },
    "num_outputs": {
      "default": 2,
      "title": "Num Outputs",
      "type": "integer",
      "ui:widget": "none"
    },
    "keywords": {
      "items": {
        "type": "string"
      },
      "title": "Keywords",
      "type": "array",
      "ui:widget": "keywords"
    },
    "data_source": {
